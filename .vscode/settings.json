{
    "dart.flutterSdkPath": ".fvm/flutter_sdk",
    // Remove .fvm files from search
    "search.exclude": {
        "**/.fvm": true
    },
    // Remove from file watching
    "files.watcherExclude": {
        "**/.fvm": true
    },
    "cSpell.words": [
        "cupertino",
        "firestore",
        "LTRB",
        "microtask",
        "readmore",
        "scrollbars",
        "toggleable",
        "vsync"
    ],
    "cSpell.enableFiletypes": [
        "dart"
    ],
    "cSpell.ignoreWords": [
        "KEEYZNDPLL",
        "Schyler",
        "appspot",
        "firebaseapp",
        "googleapis"
    ]
}